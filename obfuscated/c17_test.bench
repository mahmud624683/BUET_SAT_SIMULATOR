# c17
# 5 inputs
# 2 outputs
# 0 inverter
# 6 gates (6 NANDs)

INPUT(G1gatx)
INPUT(G2gatx)
INPUT(G3gat)
INPUT(G6gat)
INPUT(G7gat)
INPUT(keyinput0)
INPUT(keyinput1)
INPUT(keyinput2)
INPUT(keyinput3)
INPUT(keyinput4)

OUTPUT(G22gat)
OUTPUT(G23gat)

G10gat = NAND(G1gatx, G3gat)
G11gat = NAND(G3gat, G6gat)
G16gat = NAND(G2gatx, G11gat)
G19gat = NAND(G11gat, G7gat)
G22gat = NAND(G10gat, G16gat)
OPO = NAND(G16gat, G19gat)

PPI0 = NOT(G1gatx)
PPI1 = BUFF(G2gatx)
PPI2 = NOT(G3gat)
PPI3 = BUFF(G6gat)
PPI4 = NOT(G7gat)

KXOR0 = XOR(keyinput0, PPI0)
KXOR1 = XNOR(keyinput1, PPI1)
KXOR2 = XOR(keyinput2, PPI2)
KXOR3 = XNOR(keyinput3, PPI3)
KXOR4 = XNOR(keyinput4, PPI4)

W1 = AND(KXOR0, KXOR1, KXOR2, KXOR3, KXOR4)
W2 = NOT(W1)

PPI_AND = AND(PPI0, PPI1, PPI2, PPI3, PPI4)
PPI_NAND = NOT(PPI_AND)

OPO_XNOR = XNOR(OPO, PPI_NAND)
W3 = AND(W2, PPI_NAND)
K = NOT(W3)

G23gat = XOR(K, OPO_XNOR)
